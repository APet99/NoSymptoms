<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/log_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragment.LogFragment">

    <TextView
        android:id="@+id/log_date_header"
        android:layout_width="140sp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Date"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/log_results_header"
        android:layout_width="140sp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Results"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/log_link_header"
        android:layout_width="140sp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Link"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/log_layout_date"
        android:layout_width="140dp"
        android:layout_height="697dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/log_results_header"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/log_date_header"
        android:layout_marginTop="225dp"
        android:gravity="center"
        tools:context=".fragment.LogFragment"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/log_layout_results"
        android:layout_width="140dp"
        android:layout_height="697dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/log_link_header"
        app:layout_constraintStart_toEndOf="@id/log_layout_date"
        app:layout_constraintTop_toBottomOf="@+id/log_results_header"
        android:layout_marginTop="225dp"
        android:gravity="center"
        tools:context=".fragment.LogFragment" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/log_layout_link"
        android:layout_width="140dp"
        android:layout_height="697dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/log_results_header"
        app:layout_constraintTop_toBottomOf="@+id/log_date_header"
        android:layout_marginTop="225dp"
        android:gravity="center"
        tools:context=".fragment.LogFragment" />

</androidx.constraintlayout.widget.ConstraintLayout>